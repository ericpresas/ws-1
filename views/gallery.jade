doctype html
html(lang='en')
  head
    title Galeries
    meta(charset='utf-8')
    meta(name='format-detection', content='telephone=no')
    link(rel='icon', href='images/favicon.ico')
    link(rel='shortcut icon', href='images/favicon.ico')
    link(rel='stylesheet', href='stylesheets/stuck.css')
    link(rel='stylesheet', href='stylesheets/style.css')
    link(rel='stylesheet', href='stylesheets/touchTouch.css')
    script(src='javascript/jquery.js')
    script(src='javascript/jquery-migrate-1.1.1.js')
    script(src='javascript/script.js')
    script(src='javascript/superfish.js')
    script(src='javascript/jquery.equalheights.js')
    script(src='javascript/jquery.mobilemenu.js')
    script(src='javascript/jquery.easing.1.3.js')
    script(src='javascript/tmStickUp.js')
    script(src='javascript/jquery.ui.totop.js')
    script(src='javascript/touchTouch.jquery.js')
    script.
      $(document).ready(function(){
      $().UItoTop({ easingType: 'easeOutQuart' });
      $('#stuck_container').tmStickUp({});
      $('.gallery .gall_item').touchTouch();
      });
    //if lt IE 9
      div(style=' clear: both; text-align:center; position: relative;')
        a(href='http://windows.microsoft.com/en-US/internet-explorer/products/ie/home?ocid=ie6_countdown_bannercode')
          img(src='http://storage.ie6countdown.com/assets/100/images/banners/warning_bar_0000_us.jpg', border='0', height='42', width='820', alt='You are using an outdated browser. For a faster, safer browsing experience, upgrade for free today.')
      script(src='javascript/html5shiv.js')
      link(rel='stylesheet', media='screen', href='stylesheets/ie.css')
  body#top.page1
    //
      ==============================
      header
      =================================
    header
      //
        ==============================
        Stuck menu
        =================================
      section#stuck_container
        .container
          .row
            .grid_12
              h1
                a(href='/')
                  img(src='https://image.flaticon.com/icons/png/128/69/69366.png', alt='Logo alt', width='100px', height='100px')
              .navigation
                nav
                  ul.sf-menu
                    li.current
                      a(href='/') home
                    li
                      a(href='/gallery') galleries
                    li
                      a(href='/about') about us
                    li
                      a(href='/contact') contact
                .clear
    //
      =====================
      Content
      ======================
    section.content
      .ic More Website Templates @ TemplateMonster.com - July 30, 2014!
      .container
        .row
          .grid_12
            h2 Galerías
          .gallery
            each gallery in Galleries
              .grid_4
                a.gall_item(href='/galleryElements?id_gallery=' + gallery.id)
                  img(src=gallery.src, alt='')
                //a.link1(href='/galleryElements?id_gallery=' + gallery.id) +
                h1
                  !{gallery.description}
                .clear

    //
      ==============================
      footer
      =================================
    footer#footer
      .container
        .row
          .grid_12
            .socials
              a.fa.fa-twitter(href='#')
              a.fa.fa-facebook(href='#')
              a.fa.fa-google-plus(href='#')
              a.fa.fa-pinterest(href='#')
            .copyright
              span.brand Bliss 
              |  © 
              span#copyright-year
              |  
              a(href='#') Privacy Policy
              div
                | Website designed by 
                a(href='http://www.templatemonster.com/', rel='nofollow') TemplateMonster.com
